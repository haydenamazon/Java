hours = in.nextInt();

System.out.println("How many minutes?"):
minutes = in.nextInt();

System.out.println("How many seconds?");
sec = in.nextInt();

System.out.println(" ");

clock myClock = new clock(hours, minutes, sec);
myClock.advanceTime();
